pkgname="Python"
pkgversion="3.4.1"
pkgfile="${pkgname}-${pkgversion}.tar.xz"
pkguri="https://www.python.org/ftp/python/3.4.1/${pkgfile}"
pkgmd5="6cafc183b4106476dd73d5738d7f616a"

extract_source() {
  tar xfJ $(pkgdownload_path) -C /tmp
}

install_package() {
  cd ${pkgbuild_path}
  ./configure \
    --prefix="${BREW_PREFIX}/python/${pkgname}-${pkgversion}"
  make
  make install
}
